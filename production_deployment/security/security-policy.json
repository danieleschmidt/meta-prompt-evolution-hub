{
  "security_policy": {
    "encryption": {
      "at_rest": "AES-256",
      "in_transit": "TLS-1.3"
    },
    "access_controls": {
      "rbac_enabled": true,
      "network_policies": true,
      "pod_security_standards": "restricted"
    },
    "compliance": {
      "gdpr_ready": true,
      "data_retention_days": 365,
      "audit_logging": true
    }
  }
}